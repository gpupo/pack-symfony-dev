{
    "name": "gpupo/pack-symfony-dev",
    "description": "Pacote de componentes de uso comum em ambiente de desenvolvimento em projetos que rodam Symfony Framework",
    "homepage": "https://opensource.gpupo.com/pack-symfony-dev/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["common", "development"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "symfony/debug-pack": "^1.0",
        "symfony/maker-bundle": "^1.25",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "gpupo/common-dev": "^5.3 || dev-main"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\PackSymfonyDev\\": "src",
            "Gpupo\\PackSymfonyDev\\Tests\\": "tests"
        }
    },
    "replace":{
        "gpupo/pack-symfony-dev": "*"
    },
    "require-dev": {
        "gpupo/common-sdk": "^6.4 || dev-main",
        "gpupo/pack-symfony-common": "^4.8 || dev-main"
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
