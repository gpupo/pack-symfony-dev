{
    "name": "gpupo/pack-symfony-dev",
    "description": "Pacote de componentes de uso comum em ambiente de desenvolvimento em projetos que rodam Symfony Framework",
    "homepage": "https://opensource.gpupo.com/pack-symfony-dev/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["common", "development"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "gpupo/common-dev": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\PackSymfonyDev\\": "src",
            "Gpupo\\PackSymfonyDev\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "gpupo/common-sdk": "*",
        "gpupo/pack-symfony-common": "*"
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
